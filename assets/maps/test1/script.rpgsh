ADD NPC 600 600 Parler
    UPDATE
        MOVE 600 600
        MOVE 700 700
    END

ADD TRIGGER 1550 1650 Activer
    UPDATE
        IF PLAYER_IN_RANGE 100
            SET_PLAYER_MOVE 0
            CAM_TARGET
            MOVE 630 630 4
            MOVE 660 660 1
            MOVE 630 630 1
            MOVE 1550 1650 4
            CAM_TARGET PLAYER
            SET_PLAYER_MOVE 1
            DESTROY
        FI
    END